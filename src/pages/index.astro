---
import { getEntry, render } from 'astro:content';
import MainLayout from '../layouts/MainLayout.astro';
import JamedaEmbedding from '../components/JamedaEmbedding.astro';
import DocToLibButton from '../components/DocToLibButton.astro';
import Card from '../components/Card.astro';

const entry = await getEntry('page', 'index');

if (!entry) {
    return Astro.redirect('/404');
}

const { Content } = await render(entry);
---
<MainLayout {...entry.data}>
    <Content components={{JamedaEmbedding, DocToLibButton, Card}} />
</MainLayout>
